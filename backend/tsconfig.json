{
    "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "target": "ESNext",
      "outDir": "dist",
      "rootDir": "src",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }